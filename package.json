{
  "name": "simu-dos",
  "version": "1.2.2",
  "description": "Dos game for browser",
  "author": "DavidJones <qowera@qq.com>",
  "license": "MIT",
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "tslint": "tslint --config ./tslint.json --fix './src/**/*.ts' './bundler/**/*.ts'",
    "lint": "npm run tslint",
    "test": "npm run lint",
    "bundler": "tsc --project ./bundler --outDir ./libs/bundler",
    "compile": "rimraf ./dist ./.cache && npm run bundler",
    "develop": "npm run compile && node ./libs/bundler/index.js",
    "release": "npm run compile && cross-env NODE_ENV=production node ./libs/bundler/index.js",
    "start": "npm run develop"
  },
  "dependencies": {
    "@types/lodash": "4.14.122",
    "axios": "0.21.2",
    "js-dos": "6.22.19",
    "qrcode": "^1.4.1",
    "ts-md5": "^1.2.6"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.10.2",
    "@types/fs-extra": "^7.0.0",
    "@types/parcel-bundler": "^1.12.0",
    "@types/qrcode": "^1.3.4",
    "autoprefixer": "^9.5.1",
    "cross-env": "^5.2.0",
    "fs-extra": "^8.0.1",
    "lint-staged": "^7.2.2",
    "parcel-bundler": "^1.11.0",
    "postcss-pxtorem": "^4.0.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "sass": "^1.16.0",
    "tslint": "^5.12.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.3"
  }
}
